<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet-mapping>
        <servlet-name>LoginInServlet</servlet-name>
        <url-pattern>/logIn</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LoginInServlet</servlet-name>
        <servlet-class>servlets.LogInServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>UsersListServlet</servlet-name>
        <url-pattern>/usersList</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UsersListServlet</servlet-name>
        <servlet-class>servlets.UsersListServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>StocksDataServlet</servlet-name>
        <url-pattern>/stocksList</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>StocksDataServlet</servlet-name>
        <servlet-class>servlets.StocksDataServlets</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>IssueStockServlet</servlet-name>
        <url-pattern>/pages/brokerHomePage/IssueStock</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>IssueStockServlet</servlet-name>
        <servlet-class>servlets.IssueStockServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>UserDataServlet</servlet-name>
        <url-pattern>/userData</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UserDataServlet</servlet-name>
        <servlet-class>servlets.UserDataServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ChargeMoneyServlet</servlet-name>
        <url-pattern>/pages/brokerHomePage/chargeMoney</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ChargeMoneyServlet</servlet-name>
        <servlet-class>servlets.ChargeMoneyServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>uploadFileServlet</servlet-name>
        <url-pattern>/pages/brokerHomePage/uploadFile</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>uploadFileServlet</servlet-name>
        <servlet-class>servlets.filesServlets.FileUploadServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>StockInfoUpdateAdminServlet</servlet-name>
        <url-pattern>/adminStockPageUpdateData</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>StockInfoUpdateAdminServlet</servlet-name>
        <servlet-class>servlets.StockInfoUpdateAdminServlet</servlet-class>
    </servlet>

</web-app>